# The following two records generate values for the x-axis

record(aSub,"$(P):ASUB") 
{
    field(SCAN, "5 second")
    field(SNAM, "incrementor")
    field(FTA,  "DOUBLE")
	field(INPA, "$(P):XINC_CH1")
    field(OUTA, "$(P):OUT")
    field(FTVA, "DOUBLE")
    field(NOVA, "10000")
	field(NEVA, "10000")
    field(FLNK, "$(P):XDATA")
}

record(waveform, "$(P):XDATA")
{
    field(INP, "$(P):ASUB.VALA")
    field(DTYP, "Soft Channel")
    field(NELM, "10000")
    field(FTVL, "DOUBLE")
}